{
  "name": "@ui8kit/core",
  "version": "0.1.5",
  "description": "Open source React UI components library with Tailwind utility-first and semantic classes",
  "homepage": "https://buildy.tw/",
  "license": "GPL-3.0",
  "author": "buildy-ui",
  "type": "module",
  "keywords": [
    "components",
    "design",
    "frontend",
    "library",
    "tailwind",
    "utility-first",
    "semantic",
    "react",
    "ui",
    "ui8kit"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "url": "https://github.com/ui8kit/core.git",
    "type": "git"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.525.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
